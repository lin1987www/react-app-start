{
  "name": "@username/react-app-start",
  "version": "0.0.1",
  "description": "",
  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/lin1987www/react-app-start.git"
  },
  "scripts": {
    "test": "npm run test:lint && npm run test:mocha && npm run test:css",
    "test:lint": "eslint . --ext .js,.jsx",
    "test:mocha": "mocha --require @babel/register ./test/index.js src/**/*.spec.js src/**/*.spec.jsx",
    "test:css": "stylelint src/**/*.css src/**/*.scss",
    "test:jest": "jest __test__ --no-cache --watch",
    "start": "webpack-dev-server --open --config webpack.config.js",
    "build": "cross-env NODE_ENV=production NODE_OPTIONS=--max_old_space_size=4096 webpack --progress --config webpack.config.js",
    "build:dev": "cross-env NODE_ENV=development NODE_OPTIONS=--max_old_space_size=4096 webpack --progress --config webpack.config.js",
    "build:css": "postcss src/**/*.css src/**/*.scss --base src --dir dist --config postcss.config.js",
    "i18n:src": "node ./scripts/build-i18n-src.js ./translations/messages/src ./translations",
    "build:msgs": "node ./scripts/merge-messages.js ./translations"
  },
  "author": "",
  "license": "MIT",
  "files": [
    "src",
    "test",
    "translations",
    "*.js",
    ".eslintignore",
    ".travis.yml"
  ],
  "browserslist": [
    "last 3 versions",
    "Safari >= 8",
    "iOS >= 8",
    "ie >= 8"
  ],
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.2.0",
    "@babel/plugin-proposal-json-strings": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/plugin-proposal-optional-chaining": "^7.6.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-syntax-import-meta": "^7.2.0",
    "@babel/plugin-transform-arrow-functions": "^7.2.0",
    "@babel/plugin-transform-async-to-generator": "^7.4.4",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.4.4",
    "autoprefixer": "^9.6.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-dynamic-import-node": "^2.3.0",
    "babel-plugin-react-intl": "^3.3.3",
    "babel-plugin-react-intl-auto": "^1.7.0",
    "chai": "^4.2.0",
    "clean-webpack-plugin": "^1.0.1",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "detect-mocha": "^0.1.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "^5.16.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-react": "^7.14.2",
    "eslint-plugin-react-hooks": "^1.6.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.8.0",
    "jsdom": "^13.2.0",
