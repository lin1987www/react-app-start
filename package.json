{
  "name": "@username/react-app-start",
  "version": "0.0.1",
  "description": "",
  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/lin1987www/react-app-start.git"
  },
  "scripts": {
    "test": "npm run test:lint && npm run test:mocha && npm run test:css",
    "test:lint": "eslint . --ext .js,.jsx",
    "test:mocha": "mocha --require @babel/register ./test/index.js src/**/*.spec.js src/**/*.spec.jsx",
    "test:css": "stylelint src/**/*.css src/**/*.scss",
    "test:jest": "jest __test__ --no-cache --watch",
    "start": "webpack-dev-server --open --config webpack.config.js",
    "build": "cross-env NODE_ENV=production NODE_OPTIONS=--max_old_space_size=4096 webpack --progress --config webpack.config.js",
    "build:dev": "cross-env NODE_ENV=development NODE_OPTIONS=--max_old_space_size=4096 webpack --progress --config webpack.config.js",
    "build:css": "postcss src/**/*.css src/**/*.scss --base src --dir dist --config postcss.config.js"
  },
  "author": "",
  "license": "MIT",
  "files": [
    "src",
    "test",
    "*.js",
    ".eslintignore",
    ".travis.yml"
  ],
  "browserslist": [
    "last 3 versions",
    "Safari >= 8",
    "iOS >= 8",
    "ie >= 8"
  ],
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.2.0",
    "@babel/plugin-proposal-json-strings": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-syntax-import-meta": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "autoprefixer": "^9.4.5",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.5",
    "chai": "^4.2.0",
    "clean-webpack-plugin": "^1.0.0",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "eslint": "^5.12.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-react": "^7.12.4",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.6.0",
    "jsdom": "^13.1.0",
    "mocha": "^5.2.0",
    "mocha-loader": "^2.0.0",
    "postcss-cli": "^6.1.1",
    "postcss-loader": "^3.0.0",
    "postcss-safe-parser": "^4.0.1",
    "precss": "^4.0.0",
    "react-test-renderer": "^16.7.0",
    "sinon": "^7.2.2",
    "style-loader": "^0.23.1",
    "stylelint": "^9.10.0",
    "stylelint-config-recommended": "^2.1.0",
    "stylelint-webpack-plugin": "^0.10.5",
    "terser-webpack-plugin": "^1.2.1",
    "webpack": "^4.28.4",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14",
    "webpack-manifest-plugin": "^2.0.4",
    "webpack-merge": "^4.2.1"
  },
